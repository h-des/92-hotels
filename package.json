{
  "name": "92-hotels",
  "version": "1.0.0",
  "description": "92-hotels is an fake app that allows you to find a room in a hotel, check availability and book it.",
  "main": "index.js",
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h-des/92-hotels.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/h-des/92-hotels/issues"
  },
  "homepage": "https://github.com/h-des/92-hotels#readme",
  "dependencies": {
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "nodemon": "^1.18.9"
  }
}
